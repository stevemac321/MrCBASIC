SUB GetXY (x, y)
    DO
        i = _MOUSEINPUT
        IF _MOUSEBUTTON(1) THEN
            x = _MOUSEX
            y = _MOUSEY
            EXIT DO
        END IF
    LOOP
    DO
        i = _MOUSEINPUT
